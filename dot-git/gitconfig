[user]
        name = Russell MacDonald
	email = rmacdonald@arlo.com

[github]
        user = rumadon

[core]
        exludesfile = ~/.gitignore_global
	editor = nano
	excludesfile = /Users/Envoy/.gitignore_global
	autocrlf = false
	ignorecase = false

[pull]
        rebase = true

[rebase]
	autosquash = true

[alias]
        d = checkout develop
        m = checkout master
        ca = commit --all --amend
        cw = commit --all -m \"working commit\"
        rh = reset HEAD~
        rc = commit --all -c ORIG_HEAD
        po = push origin HEAD --force-with-lease
        tree = log --graph --oneline --decorate --all
	l = log
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[credential]
	helper = osxkeychain
